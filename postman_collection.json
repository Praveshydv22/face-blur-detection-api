{
  "info": {
    "name": "Face Blur Detection API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Upload Image",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "image",
              "type": "file",
              "src": "samples/input.jpg"
            }
          ]
        },
        "url": {
          "raw": "http://127.0.0.1:8000/api/blur/upload/"
        }
      }
    }
  ]
}